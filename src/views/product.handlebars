<div class="box">
    <div class="product-detail">
        <h2>{{this.title}}</h2>
        <p>{{this.description}}</p>
        <p>Price: ${{this.price}}</p>
        <p>Categoria: {{this.category}}</p>
        <p>Codigo: {{this.code}}</p>
        <p>Stock: {{this.stock}}</p>
        <button id="carrito-botton" onclick="">Agregar al Carrito</button>
    </div>
</div>
