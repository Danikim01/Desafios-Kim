<h1 style="text-align: center;margin-top: 50px;">Productos: </h1>

<div id="productos">
    {{#each products}}
        <div class="product-card">
            <h2>{{this.title}}</h2>
            <p>{{this.description}}</p>
            <p>Price: ${{this.price}}</p>
            <p>Category: {{this.category}}</p>
            <p>Code: {{this.code}}</p>
            <p>Stock: {{this.stock}}</p>
        </div>
    {{/each}}
</div>